{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FabianoFPS/api-weather_forecast_for_surf.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Fabiano F. P. Stoffel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FabianoFPS/api-weather_forecast_for_surf/issues"
  },
  "homepage": "https://github.com/FabianoFPS/api-weather_forecast_for_surf#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "eslint": "7.2.0",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "26.0.0",
    "ts-node-dev": "1.0.0-pre.47",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "@overnightjs/core": "1.7.4",
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "config": "3.3.1",
    "express": "4.17.1",
    "module-alias": "2.2.2"
  }
}
