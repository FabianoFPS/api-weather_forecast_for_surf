{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "dokcer:create-mongodb": "docker run --name surf_forecast -p 27017:27017 -d -t mongo",
    "docker:start-mongodb": "docker start surf_forecast",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/index.ts | pino-pretty -c",
    "start:dev-debug": "DEBUG=* ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/index.ts | pino-pretty -c",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "test": "yarn test:unit && yarn test:functional",
    "local-environments": ". untracked_files/environments.sh && echo \"Ã‰ necessario executar manualemnte: . untracked_files/environments.sh\""
  },
  "engines": {
    "node": "14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FabianoFPS/api-weather_forecast_for_surf.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Fabiano F. P. Stoffel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FabianoFPS/api-weather_forecast_for_surf/issues"
  },
  "homepage": "https://github.com/FabianoFPS/api-weather_forecast_for_surf#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "3.0.0",
    "@types/config": "0.0.36",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.11",
    "@types/express-pino-logger": "4.0.2",
    "@types/http-status-codes": "1.2.0",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash": "4.14.155",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.10.5",
    "@types/multer": "1.4.3",
    "@types/nock": "11.1.0",
    "@types/node": "^14.14.37",
    "@types/pino": "^6.3.0",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.2.0",
    "nock": "12.0.3",
    "pino-pretty": "4.0.0",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "1.0.0-pre.47",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "@overnightjs/core": "1.7.4",
